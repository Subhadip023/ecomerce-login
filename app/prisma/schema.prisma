datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  categories Category[]
}

model Category {
  id    Int      @id @default(autoincrement())
  name  String
  users User[]
}
